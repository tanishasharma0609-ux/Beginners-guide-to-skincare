<script>
  document.addEventListener('DOMContentLoaded', function () {
    const $slider = $('.announcement-slider-sections--19981169721580__scroll_accnomest_hMdmB9');
    $slider.on('init', function () {
      $slider.css({ visibility: 'visible', opacity: 1 });
    }).slick({
      slidesToShow: 1,
      slidesToScroll: 1,
      autoplay: true,
      autoplaySpeed: 8000,
      speed: 700,
      cssEase: 'ease',
      infinite: true,
      arrows: true,
      prevArrow: '<button type="button" class="slick-prev custom-arrow prev-arrow">&#10094;</button>',
      nextArrow: '<button type="button" class="slick-next custom-arrow next-arrow">&#10095;</button>',
      pauseOnHover: false,
      pauseOnFocus: false,
      responsive: [{ breakpoint: 768, settings: { arrows: false } }]
    });

    function startCountdowns() {
      document.querySelectorAll('.main-wrapper-ab').forEach(wrapper => {
        const countdownWrapper = wrapper.querySelector('.countdown-wrapper');
        if (!countdownWrapper) return;

        const endTime = wrapper.dataset.countdown;
        if (!endTime) return;

        const daysEl = wrapper.querySelector('.days');
        const hoursEl = wrapper.querySelector('.hours');
        const minutesEl = wrapper.querySelector('.minutes');
        const secondsEl = wrapper.querySelector('.seconds');

        function updateCountdown() {
          const now = new Date().getTime();
          const distance = new Date(endTime).getTime() - now;

          if (distance <= 0) {
            daysEl.textContent = "00";
            hoursEl.textContent = "00";
            minutesEl.textContent = "00";
            secondsEl.textContent = "00";
            return;
          }

          const days = Math.floor(distance / (1000 * 60 * 60 * 24));
          const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
          const mins = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
          const secs = Math.floor((distance % (1000 * 60)) / 1000);

          daysEl.textContent = days.toString().padStart(2, '0');
          hoursEl.textContent = hours.toString().padStart(2, '0');
          minutesEl.textContent = mins.toString().padStart(2, '0');
          secondsEl.textContent = secs.toString().padStart(2, '0');
        }

        updateCountdown();
        setInterval(updateCountdown, 1000);
      });
    }


  startCountdowns();


  });
</script>


