<script>
  document.addEventListener('DOMContentLoaded', function () {
    // Reset variant selector on load
    setTimeout(() => {
      document.querySelectorAll('.fbt-variant-selector').forEach(selector => {
        selector.selectedIndex = 0;
        selector.dispatchEvent(new Event('change', { bubbles: true }));
      });
    }, 50);

    function updateVariantUI(selector) {
      const variantId = selector.value;
      const productId = selector.dataset.productId;
      const selectedOption = selector.selectedOptions[0];
      const price = parseInt(selectedOption.dataset.price);
      const compareAt = parseInt(selectedOption.dataset.compare);
      const productEl = document.querySelector(`.fbt-product[data-product-id="${productId}"]`);
      const priceEl = productEl.querySelector(`#price-${productId}`);
      const imgEl = productEl.querySelector(`#product-image-${productId}`);
      const discountTag = productEl.querySelector('.discount-tag');
      const addBtn = productEl.querySelector('.fbt-single-add-btn');

      if (addBtn) addBtn.setAttribute('data-variant-id', variantId);

      if (compareAt > price) {
        const discount = Math.round((compareAt - price) * 100 / compareAt);
        priceEl.innerHTML = `<s class="compare-price">₹${(compareAt / 100).toFixed(2)}</s> ₹${(price / 100).toFixed(2)}`;
        if (discountTag) {
          discountTag.textContent = `-${discount}%`;
        } else {
          const tag = document.createElement('span');
          tag.className = 'discount-tag';
          tag.textContent = `-${discount}%`;
          imgEl.parentNode.appendChild(tag);
        }
      } else {
        priceEl.innerHTML = `₹${(price / 100).toFixed(2)}`;
        if (discountTag) discountTag.remove();
      }

      fetch(`/variants/${variantId}.js`)
        .then(res => res.json())
        .then(data => {
          if (data.featured_image && imgEl) {
            imgEl.src = data.featured_image.src.replace('.jpg', '_300x.jpg').replace('.png', '_300x.png');
          }
        });
    }

    document.querySelectorAll('.fbt-variant-selector').forEach(selector => {
      selector.addEventListener('change', function () {
        updateVariantUI(this);
      });
      setTimeout(() => {
        updateVariantUI(selector);
      }, 100);
    });

    document.querySelectorAll('.fbt-single-add-btn').forEach(btn => {
      btn.addEventListener('click', function () {
        const variantId = parseInt(this.getAttribute('data-variant-id'));

        fetch('/cart/add.js', {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({ items: [{ id: variantId, quantity: 1 }] })
        })
        .then(response => response.json())
        .then(data => {
          if (window.Shopify && Shopify.Storefront && Shopify.Storefront.Cart && Shopify.Storefront.Cart.openCart) {
            Shopify.Storefront.Cart.openCart();
          } else if (window.CartDrawer && typeof CartDrawer.open === 'function') {
            CartDrawer.open();
          } else {
            document.dispatchEvent(new CustomEvent('cart:refresh', { detail: { cart: data } }));
          }
        })
        .catch(err => console.error('Cart add error:', err));
      });
    });

    function initFBTSlickSlider() {
      if (window.innerWidth < 992) {
        const $fbtSlider = $('.fbt-slick');
        if (!$fbtSlider.hasClass('slick-initialized')) {
          $fbtSlider.slick({
            dots: false,
            arrows: true,
            infinite: false,
            speed: 300,
            slidesToShow: 1,
            slidesToScroll: 1
          });
        }
      } else {
        if ($('.fbt-slick').hasClass('slick-initialized')) {
          $('.fbt-slick').slick('unslick');
        }
      }
    }

    initFBTSlickSlider();
    window.addEventListener('resize', initFBTSlickSlider);
  });
</script>
